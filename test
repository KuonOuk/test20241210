    if len(sys.argv) != 5 or sys.argv[1] != '--start' or sys.argv[3] != '--end':
        print("Usage: python script_name.py --start \"YYYY/MM/DD HH:MM:SS\" --end \"YYYY/MM/DD HH:MM:SS\"")
        sys.exit(1)

    start_time = sys.argv[2]
    end_time = sys.argv[4]

    # 日時のパース
    start_time = datetime.strptime(start_time, "%Y/%m/%d %H:%M:%S")
    end_time = datetime.strptime(end_time, "%Y/%m/%d %H:%M:%S")
